<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="SatDownloadLibrary.Model" assembly="SatDownloadLibrary">

  <class name="Log">
 
    <id name="Id">
      <generator class="guid.comb" />
    </id>


    <property name="DateTime" />
    <property name="TicketId" />
    <property name="Line" type="string" length="1000000" />
    <property name="Level" />

  </class>

</hibernate-mapping>